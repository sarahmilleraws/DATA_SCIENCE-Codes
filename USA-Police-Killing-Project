   "cell_type": "code",
   "execution_count": 386,
   "metadata": {},
   "outputs": [],
   "source": [
    "seperate = kill.name[kill.name != \"TK TK\"].str.split()\n",
    "a,b = zip(*seperate)\n",
    "name_list =a+b\n",
    "name_count = Counter(name_list)\n",
    "most_common_names = name_count.most_common(15)"
   ]
  },
